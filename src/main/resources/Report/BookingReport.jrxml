	<parameter name="P_DATE" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT 
    b.bookingId,
    g.name AS guestName,
    b.guestPhone,
    b.roomType,
    b.roomId,
    b.checkIn,
    b.duration,
    b.totalPrice,
    b.status
FROM 
    booking b
JOIN 
    guest g ON b.guestPhone = g.contact
ORDER BY 
    b.checkIn DESC
]]></query>
